<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
 targetNamespace="http://www.zeeshan.com/Employee" 
 xmlns:tns="http://www.zeeshan.com/Employee" elementFormDefault="qualified">
 <element name="Employee" type="tns:Employee"/>
 <complexType name="Employee">
		 <sequence>
			 <element name="name" type="tns:String10Chars"/>
			 <element name="age" type="int"/>
			 <element name="email" type="string"/>
			 <element name="gender" type="string"/>
			 <element name="phone" type="string"/>
		 </sequence>
 </complexType>
 
 <complexType name="PaymentType">
 	<choice>
 	   <element name="cash"/>
 	    <element name="online"/>
 	</choice>
 		
 </complexType>
 
 <simpleType name="String10Chars">
			<restriction base="string">
			<maxLength value="10"/>
			</restriction>
 </simpleType>
</schema>