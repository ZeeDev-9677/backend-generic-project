package java_beginners_code;

public class BasicOJ {

	public static void main(String[] args) {
		
		//fibonacci series
		/*
		 * int a=0; int b=1; int next=0; int n=5; System.out.print(a+" "+b); for(int
		 * i=2;i<n;i++) { next=a+b; System.out.print(" "+next); a=b; b=next; }
		 */
		
		//prime number
		
		int num=10;
		for(int i=2;i<num/2;i++)
		{
			int c=1;
			if(num%i==0)
			{
				c++;
			}
			if(c>1)
			{
				System.out.println("not a prime number");
			}
			else
			{
				System.out.println("A prime number");
			}
		}
	}

}
