package interviewQuestions;

public class TrianglePattern {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n=5;
 for(int i=0;i<n;i++) {
	for(int j=0;j<n-3;j--) {
		System.out.print(" ");
		for(int k=0;k<i+1;k++)
		{
			System.out.print("*");
		}
//		System.out.println(" ");
	}
	System.out.println("\n");
 }
	}

}
