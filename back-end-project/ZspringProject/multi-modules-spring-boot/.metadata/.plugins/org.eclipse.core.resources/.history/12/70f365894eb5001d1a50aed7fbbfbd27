		//count total number of characters in string
		
		Scanner sc=new Scanner(System.in);
		String str=sc.nextLine();
		int c=0;
		System.out.println("length of string is :");
		for(int i=0;i<str.length();i++)
		{
			if(str.charAt(i)!=' ')
			{
				c++;
			}
		}
		System.out.println(c);
	}

