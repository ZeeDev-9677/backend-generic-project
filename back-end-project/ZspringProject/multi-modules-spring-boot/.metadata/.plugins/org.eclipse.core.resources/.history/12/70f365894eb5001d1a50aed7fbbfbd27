		 * emptyStr=emptyStr+str.charAt(i); }
		 * 
		 * if(str.toLowerCase().equals(emptyStr.toLowerCase())) {
		 * System.out.println("Palindrome"); } else {
		 * System.out.println("Not Palindrome"); }
		 */
		/*
		 * StringQuestion n=new StringQuestion("Zee"); StringQuestion k=new
		 * StringQuestion("Zee"); String str="Zee"; String str1="Zee";
		 * if(str.equals(str1)&&k.equals(n)) { System.out.println( "true" ); } else {
		 * System.out.println("false"); }
